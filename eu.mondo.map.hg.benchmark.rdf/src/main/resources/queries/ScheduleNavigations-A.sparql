PREFIX  base: <http://www.semanticweb.org/ontologies/2015/trainbenchmark#>
PREFIX  rdf:  <http://www.w3.org/1999/02/22-rdf-syntax-ns#>

SELECT  ?station1 ?station2 ?station3
WHERE
  { ?association rdf:type base:Association .
    ?association base:location ?station1 .
    ?station1 base:neighbors ?station2 .
    ?station2 base:neighbors ?station3
    FILTER (?station1 != ?station3) .
  }
